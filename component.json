{
  "title": "DNB API (Node.js)",
  "description": "elastic.io component for the DNB API",
  "docsUrl": "https://github.com/elasticio/dnb-component-nodejs",
  "credentials": {
    "fields": {
      "username": {
        "label": "DNB API Username",
        "required": true,
        "viewClass": "TextFieldWithNoteView",
        "note": "Obtain your username at: https://docs.dnb.com"
      },
      "password": {
        "label": "DNB API Password",
        "required": true,
        "viewClass": "TextFieldWithNoteView",
        "note": "Obtain your password at: https://docs.dnb.com"
      }
    }
  },
  "actions": {
    "getCompanyNewsREST": {
      "main": "./lib/actions/getCompanyNewsREST.js",
      "title": "Get Company News From DNB",
      "metadata": {
        "in": "./lib/schemas/getCompanyNewsREST.in.json",
        "out": "./lib/schemas/getCompanyNewsREST.out.json"
      }
    }
  }
}
